<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <option name="SMART_BACKSPACE" value="0" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="36000000" />
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="0" />
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Ubuntu Mono" />
    <option name="LINE_SPACING" value="1.15" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Ubuntu" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *scratch*" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="RENAME_VARIABLES_INPLACE" value="false" />
    <option name="PRESELECT_RENAME" value="false" />
    <option name="SHOW_INLINE_DIALOG" value="false" />
    <option name="ADD_CARETS_ON_DOUBLE_CTRL" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="RubyEditorOptions">
    <option name="startInterpolationOnHash" value="true" />
  </component>
  <component name="SqlFoldingSettings">
    <option name="COLLAPSE_LONG_NUMBERS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>